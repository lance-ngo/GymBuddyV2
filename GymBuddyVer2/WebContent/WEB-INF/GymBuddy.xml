<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xmlns="http://java.sun.com/xml/ns/javaee" 
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>MembershipServlet</servlet-name>
    <servlet-class>MembershipServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GetFeedbackServlet</servlet-name>
    <servlet-class>GetFeedbackServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SearchClassServlet</servlet-name>
    <servlet-class>SearchClassServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>StaffLoginServlet</servlet-name>
    <servlet-class>StaffLoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CreateClassServlet</servlet-name>
    <servlet-class>CreateClassServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CreateServlet</servlet-name>
    <servlet-class>CreateServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>FeedbackServlet</servlet-name>
    <servlet-class>FeedbackServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GetFeedbackServlet</servlet-name>
    <url-pattern>/GetFeedbackServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>StaffLoginServlet</servlet-name>
    <url-pattern>/StaffLoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CreateClassServlet</servlet-name>
    <url-pattern>/CreateClassServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CreateServlet</servlet-name>
    <url-pattern>/CreateServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MembershipServlet</servlet-name>
    <url-pattern>/MembershipServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SearchClassServlet</servlet-name>
    <url-pattern>/SearchClassServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FeedbackServlet</servlet-name>
    <url-pattern>/FeedbackServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>